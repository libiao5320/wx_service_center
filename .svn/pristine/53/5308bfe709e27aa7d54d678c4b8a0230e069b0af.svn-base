public class Darea extends BaseEntity implements java.io.Serializable {
    private static final long serialVersionUID = 5454155825314635342L;

    //alias
    public static final String TABLE_ALIAS = "Darea";
    public static final String ALIAS_AREA_ID = "索引ID";
    public static final String ALIAS_AREA_NAME = "地区名称";
    public static final String ALIAS_AREA_PARENT_ID = "地区父ID";
    public static final String ALIAS_AREA_SORT = "排序";
    public static final String ALIAS_AREA_DEEP = "地区深度，从1开始";
    public static final String ALIAS_AREA_REGION = "大区名称";

    //date formats

    //可以直接使用: @Length(max=50,message="用户名长度不能大于50")显示错误消息
    //columns START

    private Integer areaId;

    private String areaName;

    private Integer areaParentId;

    private Integer areaSort;

    private Boolean areaDeep;

    private String areaRegion;
    //columns END

    public Darea() {
    }

    public Darea(
            Integer areaId
    ) {
        this.areaId = areaId;
    }

    public void setAreaId(Integer value) {
        this.areaId = value;
    }

    public Integer getAreaId() {
        return this.areaId;
    }

    public void setAreaName(String value) {
        this.areaName = value;
    }

    public String getAreaName() {
        return this.areaName;
    }

    public void setAreaParentId(Integer value) {
        this.areaParentId = value;
    }

    public Integer getAreaParentId() {
        return this.areaParentId;
    }

    public void setAreaSort(Integer value) {
        this.areaSort = value;
    }

    public Integer getAreaSort() {
        return this.areaSort;
    }

    public void setAreaDeep(Boolean value) {
        this.areaDeep = value;
    }

    public Boolean getAreaDeep() {
        return this.areaDeep;
    }

    public void setAreaRegion(String value) {
        this.areaRegion = value;
    }

    public String getAreaRegion() {
        return this.areaRegion;
    }

}

